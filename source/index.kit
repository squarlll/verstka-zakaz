	<!-- artwork -->
	<div class="artwork">
		<div class="artwork__content">
			<div class="artwork__text">
				Это произведение искусства для роста вашего бизнеса
			</div>
			<div class="artwork__info">
				<div class="artwork__name">Клиент Тюнинг ателье Оук Фелдер</div>
				<a href="#" class="artwork__history">Почитать историю</a>
			</div>
		</div>
	</div>

	<!-- scripts -->
	<script src="js/jquery-3.4.1.min.js"></script>
	<script src="js/5crm.js"></script>
</body>
